import java.util.Scanner;

class Main {
    public static class Node {
        int data;
        Node prev;
        Node next;
    }
    
    public static Node head = null;

    public static void append(int data) {
        Node newNode = new Node();
        newNode.data = data;

        if (head == null) {
            head = newNode;
        } else {
            Node temp = head;
            while (temp.next != null) temp = temp.next;
            temp.next = newNode;
            newNode.prev = temp;
        }
    }

    public static void display() {
        Node temp = head;
        while (temp != null) {
            System.out.print(temp.data + " ");
            temp = temp.next;
        }
        System.out.println();
    }

    public static void insertF(int data) {
        Node newNode = new Node();
        newNode.data = data;
        newNode.next = head;
        if (head != null)
            head.prev = newNode;
        head = newNode;
    }

    public static void insertE(int data) {
        append(data); 
    }

    public static void insertP(int data, int pos) {
        Node newNode = new Node();
        newNode.data = data;

        Node temp = head;
        for (int i = 1; i < pos - 1 && temp != null; i++)
            temp = temp.next;

        if (temp == null || temp.next == null)
            return;

        newNode.next = temp.next;
        newNode.prev = temp;
        temp.next.prev = newNode;
        temp.next = newNode;
    }

    public static void deleteF() {
        if (head == null) return;
        head = head.next;
        if (head != null)
            head.prev = null;
    }

    public static void deleteE() {
        if (head == null) return;
        if (head.next == null) {
            head = null;
            return;
        }

        Node temp = head;
        while (temp.next != null)
            temp = temp.next;

        temp.prev.next = null;
    }

    public static void deleteP(int data) {
        if (head == null) return;

        Node temp = head;
        while (temp != null && temp.data != data)
            temp = temp.next;

        if (temp == null) return; 

        if (temp == head) {
            deleteF();
            return;
        }
        if (temp.next == null) {
            deleteE();
            return;
        }

        temp.prev.next = temp.next;
        temp.next.prev = temp.prev;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int data;
        do {
            data = sc.nextInt();
            if (data > 0)
                append(data);
        } while (data > 0);

        display();

        insertF(10);
        insertE(15);
        insertP(20, 3);
        display();

        deleteF();
        deleteE();
        deleteP(20);
        display();
    }
}
