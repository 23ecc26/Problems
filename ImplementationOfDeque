public class Main
{
    public static class Node {
        int data;
        Node next;
        Node prev; 
        
        public Node(int data) {
            this.data = data;
            this.next = null;
            this.prev = null;
        }
    }
    
    public static Node head = null; 
    public static Node tail = null; 
    
    public static boolean isEmpty() {
        return head == null;
    }

    public static void insertRear(int data) {
        Node newNode = new Node(data);

        if (isEmpty()) {
            head = tail = newNode;
        } else {
            newNode.prev = tail;
            tail.next = newNode;
            tail = newNode;
        }
    }

    public static void insertFront(int data) {
        Node newNode = new Node(data);

        if (isEmpty()) {
            head = tail = newNode;
        } else {
            newNode.next = head;
            head.prev = newNode;
            head = newNode;
        }
    }

    public static void deleteFront() {
        if (isEmpty()) {
            return;
        }
        
        if (head == tail) {
            head = tail = null;
        } else {
            head = head.next;
            head.prev = null;
        }
    }
    
    public static void deleteRear() {
        if (isEmpty()) {
            return;
        }
        
        if (head == tail) {
            head = tail = null;
        } else {
            tail = tail.prev;
            tail.next = null;
        }
    }

    public static void display() {
        if (isEmpty()) {
            System.out.println("Deque is Empty.");
            return;
        }
        
        Node temp = head;
        while(temp != null) {
            System.out.print(temp.data + " ");
            temp = temp.next;
        }
        System.out.println();
    }
	public static void main(String[] args) {
		insertRear(10);
        insertFront(5);
        insertRear(15);
        display();
        deleteFront();
        deleteRear();
        display();
		
	}
}
