import java.util.Scanner;
class Main {
    public static class Node {
          int data;
          Node next;
    }
    public static class Stack {
    public static Node head = null;
    public static void push(int data) {
        Node newNode = new Node();
        newNode.data = data;
        newNode.next = head;
        head = newNode;
    }
    public static int pop() {
            if(head == null) {
                return -1;
            }
            int val = head.data;
            Node temp = head;
            head = head.next;
            temp.next = null;
            return val;
    }
    }
    public static int solve(int a, int b, char o) {
        if(o == '+') {
            return b + a;
        }
        else if(o == '-') {
            return b - a;
        }
        else if(o == '*') {
            return b * a;
        }
        else {
            return b / a; 
        }
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Stack st = new Stack();
        String str = "2 3 + 12 * 2 /";
        String[] s = str.split(" ");
        for(String i : s) {
            if(Character.isDigit(i.charAt(0))) {
                int num = Integer.parseInt(i);
                 st.push(num);
            }
            else {
                char oper = i.charAt(0);
                int a = st.pop(); 
                int b = st.pop(); 
                int res = solve(a, b, oper);
                st.push(res);
            }
        }
        System.out.println(st.pop());
    }
}
